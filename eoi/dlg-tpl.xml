	<div JS-class="'tab-content tab-content'+this.$key+' _it'" JS-id="'tab-content'+this.$key+'_it'">
		<label>Titolo</label><input type="text" name="doctitle" it3="data" placeholder="name" JS-value="this.doctitle" style="width:60%;vertical-align: baseline;"/><br>
	<div class="multieditables" style="margin-left:10px;margin-right:15px;display:block;">
	<label>Descrizione</label><br/><div JS-id="this.$key+'-content'" name="content" it3="data" class="acedit" style="background-color:#fff;display:inline-block;min-width:450px;width:100%;height:20vh;overflow:auto;resize:both"><!--JS{this.content||''}JS--></div><br/>
	</div><textarea name="rels" it3="data:json" style="display:none" ><!--JS{JSON.stringify(this.rels||false)}JS--></textarea></div>
</dialog>